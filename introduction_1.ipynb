   "source": [
    "import math\n",
    "\n",
    "def vect_length(vect):\n",
    "\n",
    "    vect_length_squared = 0\n",
    "\n",
    "    for i in vect:\n",
    "        vect_length_squared += (i **2 )\n",
    "    \n",
    "    return math.sqrt(vect_length_squared)\n",
    "    \n",
    "test_vect = [3, 4, 12]\n",
    "\n",
    "print(f'Длина вектора {test_vect} равна {vect_length(test_vect)}')"
   ]
